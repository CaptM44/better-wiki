{
    "name": "Wiki++",
    "version": "0.0.3",
    "browser_action": {
        "default_icon": "images/icon-128.png"
    },
    "icons": {
        "128": "images/icon-128.png"
    },
    "content_scripts": [{
        "matches": ["*://*.wikipedia.org/*"],
        "js": ["script.js"],
        "run_at": "document_start"
    }],
    "web_accessible_resources": [
        "images/*.png",
        "styles/*.css"
    ],
    "manifest_version": 2
}